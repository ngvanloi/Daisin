@model List<ContactUI>

<section id="contact" class="contact">
	<div class="container">

		<div class="section-title">
			<span>Contact</span>
			<h2>Contact</h2>
			<p>Sit sint consectetur velit quisquam cupiditate impedit suscipit alias</p>
		</div>

		<div class="row">
			<div class="d-flex align-items-stretch">
				<div class="info">
					@foreach (var item in Model)
					{
						<div class="address">
							<i class="bi bi-geo-alt"></i>
							<h4>Location:</h4>
							<p>@item.Location</p>
						</div>

						<div class="email">
							<i class="bi bi-envelope"></i>
							<h4>Email:</h4>
							<p>@item.Email</p>
						</div>

						<div class="phone">
							<i class="bi bi-phone"></i>
							<h4>Call:</h4>
							<p>@item.Call</p>
						</div>

						<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d31349.47508739421!2d106.64589340342222!3d10.835447516957778!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x317529a3be3265e9%3A0x73eb335ab4f71bbb!2sGo%20Vap%20Floral%20Village%20Park!5e0!3m2!1sen!2s!4v1718248902982!5m2!1sen!2s"
								style="border:0;width:100%;height:280px;" allowfullscreen=""
								loading="lazy"
								referrerpolicy="no-referrer-when-downgrade"></iframe>
					}
				</div>

			</div>

		</div>

	</div>
</section>